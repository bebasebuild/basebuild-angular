{
  "name": "basebuild-angular-legacy",
  "version": "1.0.0.alpha.0",
  "description": "Build tool for AngularJS projects as a node module to be reusable and able to update. Write once, use everywhere! ;)",
  "main": "dist/core/index.js",
  "module": "dist/core/index.js",
  "types": "dist/types/core/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/core/index.js",
      "types": "./dist/types/core/index.d.ts",
      "require": "./dist/core/index.js"
    },
    "./core/*": {
      "import": "./dist/core/*",
      "types": "./dist/types/core/*",
      "require": "./dist/core/*"
    },
    "./plugins": {
      "import": "./dist/plugins/index.js",
      "types": "./dist/types/plugins/index.js",
      "require": "./dist/plugins/index.js"
    },
    "./plugins/*": {
      "import": "./dist/plugins/*",
      "types": "./dist/types/plugins/*",
      "require": "./dist/plugins/*"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/types/utils/index.js",
      "require": "./dist/utils/index.js"
    },
    "./utils/*": {
      "import": "./dist/utils/*",
      "types": "./dist/types/utils/*",
      "require": "./dist/utils/*"
    },
    "./types/*": {
      "types": "./dist/types/*"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimeoutZero/BaseBuildAngular.git"
  },
  "dependencies": {
    "@bebasebuild/basebuild": "3.0.0-alpha.0",
    "coffeescript": "2.7.0",
    "debug": "4.3.4"
  },
  "keywords": [
    "basebuild",
    "build",
    "base",
    "angularJS",
    "angular",
    "timeoutzero",
    "tz",
    "basebuild-angular"
  ],
  "license": "MIT",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && yarn build:eslint",
    "prepublishOnly": "yarn build",
    "dev": "tsc -w",
    "prelink": "yarn build",
    "test": "echo no tests! && exit 1"
  },
  "devDependencies": {
    "@types/node": "20.11.28",
    "shx": "0.3.4",
    "typescript": "5.2.2",
    "vitest": "2.0.3",
    "@vitest/ui": "2.0.3",
    "@tsconfig/node20": "20.1.2"

  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=20.10.0"
  }
}
